{
  "typeName": "EQ::MONITOR::NAGIOS",
  "description": "Nagios monitoring server in AWS",
  "properties": {
    "Name": {
      "type": "string",
      "description": "Name of the Nagios server."
    },
    "Id": {
      "type": "string",
      "description": "Instance Id of the nagios server."
    },
    "SubnetId": {
      "type": "string",
      "description": "Subnet Id where server is deployed. If not specified uses default VPC and default subnet, which must exist."
    },
    "SecurityGroupId": {
      "type": "string",
      "description": "Security group attached to Nagios server. If using SecurityGroup, SubnetId is required."
    }
  },
  "additionalProperties": false,
  "required": [
  ],
  "readOnlyProperties": [
    "/properties/Id"
  ],
  "primaryIdentifier": [
    "/properties/Id"
  ],
  "additionalIdentifiers": [
    [
        "/properties/Name"
    ]
  ],  
  "handlers": {
    "create": {
      "permissions": [
        "ec2:*",
        "ssm:GetParameter",
        "ssm:PutParamter",
        "logs:CreateLogStream",
        "logs:DescribeLogGroups",
        "logs:PutMetricData",
        "logs:PutLogEvents"
      ]
    },
    "read": {
      "permissions": [
        "ssm:GetParameter",
        "logs:CreateLogStream",
        "logs:DescribeLogGroups",
        "logs:PutMetricData",
        "logs:PutLogEvents"
      ]
    },
    "delete": {
      "permissions": [
        "ec2:*",
        "ssm:DeleteParameter",
        "logs:CreateLogStream",
        "logs:DescribeLogGroups",
        "logs:PutMetricData",
        "logs:PutLogEvents"
      ]
    }
  }
}
